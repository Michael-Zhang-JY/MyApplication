            android:gravity="center"
            android:text="@string/ProName"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TextbackGroundTwo"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="5dp"
            />

        <TextView
            android:id="@+id/textHotSaleOne"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:text="HOT SALE"
            android:textColor="@android:color/white"
            android:fontFamily="@font/pingfangbold"
            android:gravity="center"
            android:visibility="gone"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@+id/TextbackGroundOne"/>

        <TextView
            android:id="@+id/textHotSaleTwo"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:fontFamily="@font/pingfangbold"
            android:text="热卖"
            android:visibility="gone"
            android:textColor="@android:color/white"
            android:gravity="center"
            android:textSize="20sp"
